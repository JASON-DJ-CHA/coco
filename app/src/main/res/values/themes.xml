<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.Testcoco" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/red</item>
        <item name="colorPrimaryVariant">@color/red</item>
        <item name="colorOnPrimary">@color/red</item>

        <item name="colorSecondary">@color/blue</item>
        <item name="colorSecondaryVariant">@color/blue</item>
        <item name="colorOnSecondary">@color/blue</item>

        <!-- <item name="colorPrimary">@color/my_light_primary</item> -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Testcoco" parent="Theme.SplashScreen">
        <item name="windowSplashScreenBackground">@color/red</item>
        <item name="windowSplashScreenAnimatedIcon">@drawable/graph</item>
        <item name="postSplashScreenTheme">@style/Base.Theme.Testcoco</item>
    </style>
</resources>